{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "scamnemesis/search",
    "version": "1.0.0",
    "title": "Scamnemesis Search",
    "category": "widgets",
    "icon": "search",
    "description": "Search the Scamnemesis fraud database.",
    "keywords": ["search", "fraud", "scam", "scamnemesis"],
    "supports": {
        "html": false,
        "align": ["wide", "full"]
    },
    "textdomain": "scamnemesis",
    "attributes": {
        "placeholder": {
            "type": "string",
            "default": ""
        },
        "buttonText": {
            "type": "string",
            "default": "Search"
        },
        "searchType": {
            "type": "string",
            "default": "all"
        },
        "showFilters": {
            "type": "boolean",
            "default": true
        },
        "resultsPerPage": {
            "type": "number",
            "default": 10
        }
    },
    "render": "file:./render.php",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./editor.css",
    "style": "file:./style.css"
}
